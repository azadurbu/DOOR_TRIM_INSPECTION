<UserControl x:Class="DOOR_TRIM_INSPECTION.Controls.CtrlInspectionSummary"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DOOR_TRIM_INSPECTION.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="120" Width="384">
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="0.30*" />
      <RowDefinition Height="0.23*" />
      <RowDefinition Height="0.23*" />
      <RowDefinition Height="0.23*" />
    </Grid.RowDefinitions>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="0.3*" />
      <ColumnDefinition Width="0.2*"/>
      <ColumnDefinition Width="0.3*" />
      <ColumnDefinition Width="0.2*"/>
    </Grid.ColumnDefinitions>
    <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" Content="{DynamicResource TXT_INSPECTION_SUMMARY}"  Style="{StaticResource InspectionSummaryTitle}"></Label>
    <Label Grid.Row="1" Grid.Column="0" Content="{DynamicResource TXT_INSPECTION_ITEM_SCREW}" Style="{StaticResource InspectionSummaryItemLabel}"></Label>
    <Label Grid.Row="1" Grid.Column="1" Content="136/136" Style="{StaticResource InspectionSummaryItemValue}" Name="lblScrewNum"></Label>
    <Label Grid.Row="1" Grid.Column="2" Content="{DynamicResource TXT_INSPECTION_ITEM_PAD}" Style="{StaticResource InspectionSummaryItemLabel}"></Label>
    <Label Grid.Row="1" Grid.Column="3" Content="136/136" Style="{StaticResource InspectionSummaryItemValue}" Name="lblPadNum"></Label>
    <Label Grid.Row="2" Grid.Column="0" Content="{DynamicResource TXT_INSPECTION_ITEM_SPEAKER}" Style="{StaticResource InspectionSummaryItemLabelAlt}"></Label>
    <Label Grid.Row="2" Grid.Column="1" Content="136/136" Style="{StaticResource InspectionSummaryItemValueAlt}" Name="lblSpeakerNum"></Label>
    <Label Grid.Row="2" Grid.Column="2" Content="{DynamicResource TXT_INSPECTION_ITEM_LEADWIRE}" Style="{StaticResource InspectionSummaryItemLabelAlt}"></Label>
    <Label Grid.Row="2" Grid.Column="3" Content="136/136" Style="{StaticResource InspectionSummaryItemValueAlt}" Name="lblLeadWireNum"></Label>
    <Label Grid.Row="3" Grid.Column="0" Content="{DynamicResource TXT_INSPECTION_ITEM_FASTENER}" Style="{StaticResource InspectionSummaryItemLabel}"></Label>
    <Label Grid.Row="3" Grid.Column="1" Content="136/136" Style="{StaticResource InspectionSummaryItemValue}" Name="lblFastenerNum"></Label>
    <Label Grid.Row="3" Grid.Column="2" Content="{DynamicResource TXT_INSPECTION_ITEM_FUSION}" Style="{StaticResource InspectionSummaryItemLabel}"></Label>
    <Label Grid.Row="3" Grid.Column="3" Content="136/136" Style="{StaticResource InspectionSummaryItemValue}" Name="lblFusionNum"></Label>
  </Grid>
</UserControl>
